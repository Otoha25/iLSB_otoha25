{
	"manifest_version": 2,
	"name": "FS_iLSB",
	"version": "1.2.5",
	"applications": {
		"gecko": {
			"id": "KeywordMap@wlgate.inf.uec.ac.jp",
			"strict_min_version": "52.0"
		}
	},

	"sidebar_action": {
		"default_title": "iLSB",
		"default_panel": "repository.html"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content-scripts.js"]
		}
	],

	"permissions": ["tabs", "storage", "activeTab"]
}

{
	"manifest_version": 2,
	"name": "iLSB_Std25",
	"version": "2025.0.0",
	"applications": {
		"gecko": {
			"id": "iLSB-Std25@wlgate.inf.uec.ac.jp",
			"strict_min_version": "52.0"
		}
	},
	"sidebar_action": {
		"default_title": "iLSB",
		"default_panel": "repository.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content-scripts.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"tabs",
		"storage",
		"activeTab",
		"contextMenus"
	]
}